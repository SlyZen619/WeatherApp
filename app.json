{
  "expo": {
    "name": "Weather App",
    "slug": "weather-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/partlycloudy.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/partlycloudy.png",
      "resizeMode": "contain",
      "backgroundColor": "#0bb3b2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/partlycloudy.png",
        "backgroundColor": "#0bb3b2"
      },
      "package": "com.solution247.weatherapp",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/partlycloudy.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7ef50f42-84b7-4651-a0c9-9240bc0f68ec"
    }
  }
}
